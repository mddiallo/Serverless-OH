{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "authLevel": "function",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "post"
      ]
    },
    {
        "type": "cosmosDB",
        "direction": "out",
        "name": "doc",
        "databaseName": "ratingsdb",
        "collectionName": "ratings",
        "createIfNotExists": "true",
        "connectionStringSetting": "AzureCosmosDBConnectionString"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "$return"
    }
  ]
}
